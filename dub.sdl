name "d_hdf5"
description "HDF5 bindings and wrappers for the D Programming Language."
homepage "https://github.com/Laeeth/d_hdf5"
authors "Laeeth Isharc"
license "Boost-1.0"
systemDependencies "requires HDF5 1.8.15.patch-1 versions of libhdf5 and libhdf5_hl"
targetType "sourceLibrary"
libs "hdf5" "hdf5_hl"
sourcePaths "source/hdf5"
preGenerateCommands "d++ --keep-d-files --preprocess-only --include-path /usr/include/hdf5 $PACKAGE_DIR/source/hdf5/wrap.dpp $PACKAGE_DIR/source/hdf5/wraphl.dpp"
